@model IEnumerable<In_Mgmt.Models.DisplayUOM>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            DUOM_Name
        </th>
        <th>
            DUOM_Code
        </th>
        <th>
            Factor
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DUOM_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DUOM_Code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Factor)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.DUOMID }) |
            @Html.ActionLink("Details", "Details", new { id=item.DUOMID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.DUOMID })
        </td>
    </tr>
}

</table>
